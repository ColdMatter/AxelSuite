             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:ni="http://schemas.ni.com/controls/2009/xaml/presentation"
             xmlns:local="clr-namespace:Axel_hub.PanelsUC"
             mc:Ignorable="d">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="35"/>
            <RowDefinition Height="386*"/>
            <RowDefinition Height="386*"/>
        </Grid.RowDefinitions>
        <StackPanel Orientation="Horizontal">
            <Label x:Name="label" Content="S-G filter" Margin="0,3,0,0.241" HorizontalAlignment="Left" Width="61"/>
            <ni:RadixNumericTextBoxInt32 x:Name="numSGfilter" Height="24" Width="43" Margin="0" HorizontalAlignment="Left" RadixVisibility="Collapsed"/>
        </StackPanel>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Margin="97,25.759,0,0" Grid.Row="1" VerticalAlignment="Top" Width="75" Click="button_Click"/>
    </Grid>
</UserControl>
