                                <ni:Graph.Plots>
                                    <ni:Plot x:Name="plotMems" Label="MEMS">
                                        <ni:PlotRendererGroup>
                                            <ni:LinePlotRenderer Stroke="#FF0A14C1" StrokeThickness="1.5"/>
