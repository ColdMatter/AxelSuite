            <RichTextBox x:Name="tbLog" Grid.Row="2" Margin="0,0.286,0,0" VerticalScrollBarVisibility="Auto" ScrollViewer.CanContentScroll="True" AcceptsReturn="True"/>
            <StackPanel HorizontalAlignment="Left" Height="30" Grid.Row="1" VerticalAlignment="Top" Width="265" Grid.RowSpan="2" Orientation="Horizontal" Background="#FFF9FEFF">
