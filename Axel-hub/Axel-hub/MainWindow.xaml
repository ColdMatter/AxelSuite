                                        </StackPanel>
                                    </GroupBox>
                                    <Button x:Name="btnClearFringes" Content="Clear Fringes/Trend" Margin="12,8,8,0" Click="btnClearAll_Click" Height="23" Background="#FFFCFCE3" />
                                </StackPanel>

