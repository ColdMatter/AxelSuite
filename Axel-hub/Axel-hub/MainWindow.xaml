            </StackPanel>
            <StackPanel Margin="0" Grid.Row="3" Orientation="Horizontal">
                <ni:LED x:Name="ledAxelShow" Content="LED" FalseContent="Off" Height="23" TrueContent="On" Width="23" Margin="10,4,0,4"/>
                <CheckBox x:Name="chkAxelShow" Content="Connect to Axel Show" VerticalAlignment="Center" Margin="7,0,0,0" Checked="chkAxelShow_Checked" Unchecked="chkAxelShow_Checked"/>
            </StackPanel>
        </Grid>
        <Grid x:Name="gridRight" Grid.Column="1" Margin="0">
